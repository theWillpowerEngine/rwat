{
  "name": "rwat",
  "version": "0.0.1",
  "description": "Riven World: Airship Trader",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "npx electron-packager . rwat --platform=win32 --arch=x64 --overwrite --icon=./stuff/icon.ico"
  },
  "build": {
    "extraResources": [
      {
        "from": "./stuff/",
        "to": "stuff",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "./unclesJournal/",
        "to": "unclesJournal",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "./zelazny/",
        "to": "zelazny",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "author": "Dan Larsen",
  "license": "ISC",
  "dependencies": {
    "color": "^4.0.1",
    "electron": "^15.3.0",
    "rng": "^0.2.2"
  }
}
